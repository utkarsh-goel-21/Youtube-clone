{
  "name": "youtube-clone",
  "version": "1.0.0",
  "description": "A YouTube clone with video streaming, user authentication, and social features",
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "video",
    "streaming",
    "social",
    "platform"
  ],
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "form-data": "^4.0.4",
    "socket.io-client": "^4.8.1"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
